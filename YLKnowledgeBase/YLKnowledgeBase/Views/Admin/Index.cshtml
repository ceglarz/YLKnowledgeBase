@model IEnumerable<ApplicationUser>
<div class="bg-primary panel-body"><h4>User Accounts</h4></div>
<table class="table table-condensed table-bordered">
    <tr><th>Name</th><th>Email</th><th>Operation</th></tr>
    @if (Model.Count() == 0)
    {
        <tr><td colspan="3" class="text-center">No User Accounts</td></tr>
    }
    else
    {
        foreach (ApplicationUser user in Model)
        {
            <tr>
               
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>
                    <a class="btn btn-sm btn-primary" asp-action="Delete" asp-route-id="@user.Id">Delete</a>
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-id="@user.Id">Edit</a>                       
                </td>


            </tr>
        }
    }
</table>
<a class="btn btn-primary" asp-action="Create">Create</a>